function [data,dRGC] = loadCurcio(fname,radDeg)

fileID = fopen(fname,'r');
C = textscan(fileID,'%f %s %f %f %f');
fclose(fileID);

R = 11.459;

[X Y] = pol2cart(deg2rad(C{4}),deg2rad(C{3}));
[X1 Y1] = pol2cart(C{4},deg2rad(C{3}));
F = scatteredInterpolant([X Y],C{5});
F.Method = 'natural';
F.ExtrapolationMethod='none';


lin = -round():stepSize:round(14);

[x,y] = meshgrid(lin,lin);
c=((x.^2+y.^2).^.5<=15);

x(c~=1) = nan;
y(c~=1) = nan;
dRGC = F(x,y);


end

